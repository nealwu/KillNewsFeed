{
  "content_scripts": [
    {
      "js": [
        "jquery.min.js",
        "kill.js"
      ],
      "matches": [
        "*://*.facebook.com/*"
      ],
      "run_at": "document_start"
    }
  ],
  "description": "Increases productivity by replacing Facebook timeline with photo and inspirational quote",
  "icons": { },
  "manifest_version": 2,
  "name": "BetterTimeline",
  "options_page": "options.html",
  "permissions": [
    "storage"
  ],
  "web_accessible_resources": [
    "images/*",
    "quotes.json"
  ],
  "version": "0.1.0"
}